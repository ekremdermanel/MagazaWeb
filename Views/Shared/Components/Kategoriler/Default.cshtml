@model List<string>

<!-- Kategoriler -->
<div class="card mb-4">
    <div class="card-header">Kategoriler</div>
    <div class="card-body p-0">
        <div class="list-group-flush">
          @foreach (var item in Model)
          {
            <a class="list-group-item" href="#!">@item</a>
          }
        </div>
    </div>
</div>